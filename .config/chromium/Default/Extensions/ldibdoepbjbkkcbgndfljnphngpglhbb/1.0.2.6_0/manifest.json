{
   "app": {
      "background": {
         "page": "background.html",
         "persistent": false
      }
   },
   "description": "mydlink services plugin",
   "externally_connectable": {
      "matches": [ "*://*.mydlink.com/*", "*://*.mydlink.net.cn/*" ]
   },
   "icons": {
      "128": "Icon_128x128.png",
      "16": "Icon_16x16.png",
      "48": "Icon_48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "mydlink services plugin",
   "permissions": [ "storage", {
      "socket": [ "tcp-connect", "tcp-listen:*:*", "resolve-host", "udp-send-to:*:*", "udp-bind:*:*" ]
   }, "browser" ],
   "short_name": "mydlink plugin",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2.6"
}
