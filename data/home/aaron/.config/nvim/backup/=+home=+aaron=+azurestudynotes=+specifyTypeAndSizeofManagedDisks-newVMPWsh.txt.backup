# Get a list of Publishers first
Get-AzVMImagePublisher -Location eastus | where-Object{$_.PublisherName -like "*linux*"}
# or (Slower)
Get-AzVMImagePublisher -Location eastus | where-Object{$_.PublisherName -like "*Canonical*"} | Select-Object PublisherName, Location

# Get managed disks offer for windows server
Get-AzVMImageOffer -Location 'EastUS' -PublisherName "MicrosoftWindowsServer"
# or Get for Ubuntu server
Get-AzVMImageOffer -Location 'EastUS' -PublisherName "canonical"

